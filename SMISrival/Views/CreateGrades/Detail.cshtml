                <input disabled asp-for="CodeTeacher" class="form-control" />
                <span asp-validation-for="CodeTeacher" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-3">
                <label asp-for="Subjects"></label>
            </div>
            <div class="col-6">
