            <div class="col-3">
                <label asp-for="Id"></label>
            </div>
            <div class="col-6">
                <input disabled asp-for="Id" asp-action="" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-3">
                <label asp-for="PersonalId"></label>
